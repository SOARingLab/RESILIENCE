{
  "name": "bpmn-js-example-properties-panel",
  "version": "0.0.0",
  "description": "A bpmn-js modeler + properties panel example",
  "main": "app/index.js",
  "scripts": {
    "all": "run-s bundle",
    "dev": "run-s bundle:less watch:js",
    "start": "run-s bundle serve",
    "serve": "sirv public --dev --port 8080",
    "bundle": "run-s bundle:*",
    "bundle:less": "lessc src/app.less public/app.css",
    "bundle:js": "webpack -c webpack.config.js",
    "watch:js": "run-s \"bundle:js -- --watch\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/bpmn-js-examples",
    "directory": "properties-panel"
  },
  "keywords": [
    "bpmnjs-example",
    "bpmn-js-properties-panel"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@bpmn-io/properties-panel": "^0.11.0",
    "bpmn-js": "^9.0.3",
    "bpmn-js-properties-panel": "^1.0.0-alpha.5",
    "jquery": "^3.5.1",
    "min-dash": "^3.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-react-jsx": "^7.16.5",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^6.5.1",
    "less": "^4.1.0",
    "less-loader": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "sirv-cli": "^1.0.10",
    "style-loader": "^3.3.1",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0"
  }
}
