mdp

global s0 : [0..12] init 0;


module M0
    [] s0=0 -> (s0'=1);
    [] s0=1 & true -> (s0'=2);
    [] s0=1 & true -> (s0'=4);
    [] s0=1 & true -> (s0'=6);
    [] s0=2 -> (s0'=3);
    [] s0=3 -> (s0'=12);
    [] s0=4 -> (s0'=5);
    [] s0=5 -> (s0'=12);
    [] s0=6 & true -> (s0'=7);
    [] s0=6 & true -> (s0'=9);
    [] s0=7 -> (s0'=8);
    [] s0=8 -> (s0'=11);
    [] s0=9 -> (s0'=10);
    [] s0=10 -> (s0'=11);
    [] s0=11 -> (s0'=12);
    [] s0=12 -> true;
endmodule

system
    M0
endsystem

