mdp

global s0 : [0..12] init 0;

global v1 : [1..10] init 2;
global v2 : [2..20] init 4;
global v3 : bool init true;

module M0
    [] s0=0 -> (s0'=1) & (v1'=v2+10);
    [] s0=1 -> (s0'=2) & (v2'=5);
    [] s0=1 -> (s0'=2) & (v2'=6);
    [] s0=1 -> (s0'=2) & (v2'=7);
    [] s0=1 -> (s0'=2) & (v2'=8);
    [] s0=2 -> 1/4 : (s0'=3) & (v2'=15)
        + 1/4 : (s0'=3) & (v2'=16)
        + 1/4 : (s0'=3) & (v2'=17)
        + 1/4 : (s0'=3) & (v2'=18)
        ;
    [] s0=3 -> (s0'=4);
    [] s0=4 -> (s0'=5);
    [] s0=5 & v1>6 -> (s0'=6);
    [] s0=5 & v2>12 -> (s0'=8);
    [] s0=5 & v3=true -> (s0'=10);
    [] s0=6 -> (s0'=7);
    [] s0=7 -> (s0'=12);
    [] s0=8 -> (s0'=9);
    [] s0=9 -> (s0'=12);
    [] s0=10 -> (s0'=11);
    [] s0=11 -> (s0'=12);
    [] s0=12 -> true;
endmodule

system
    M0
endsystem

